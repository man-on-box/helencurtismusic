---
import { getEntry } from "astro:content";
import PageLayout from "../layouts/PageLayout.astro";
import headerImage from "../assets/images/georgian-painting-800-800.webp";

const { data, render } = await getEntry("pages", "contact");
const { Content } = await render();
---

<PageLayout
  title={data.heading}
  banner={{
    heading: data.heading,
    subheading: data.subheading,
    imgSrc: headerImage,
    imgAlt: "Georgian painting",
    imgWidth: 400,
    imgHeight: 400,
  }}
>
  <article class="prose">
    <Content />
  </article>
</PageLayout>
