---
import { getEntry } from "astro:content";
import PageLayout from "../layouts/PageLayout.astro";
import headerImage from "../assets/images/helen-vocal-293-293.webp";

const { data, render } = await getEntry("pages", "music");
const { Content } = await render();
---

<PageLayout
  title={data.heading}
  banner={{
    heading: data.heading,
    imgSrc: headerImage,
    imgAlt: "Helen Curtis",
    imgWidth: 293,
    imgHeight: 293,
  }}
>
  <article class="prose">
    <Content />
  </article>
</PageLayout>
